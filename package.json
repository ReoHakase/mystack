{
  "name": "mystack",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint": "^8.34.0",
    "eslint-config-custom": "workspace:*",
    "eslint-config-prettier": "^8.6.0",
    "prettier": "^2.8.4",
    "scaffdog": "^2.4.0",
    "turbo": "^1.8.1"
  },
  "engines": {
    "node": ">=18.0.0 <19",
    "pnpm": ">=7.0.0"
  },
  "packageManager": "pnpm@7.27.1",
  "volta": {
    "node": "18.14.2"
  }
}
