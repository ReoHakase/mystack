{
  "name": "mystack",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "prepare": "husky install",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write ."
  },
  "devDependencies": {
    "eslint": "8.51.0",
    "eslint-config-custom": "workspace:*",
    "eslint-config-prettier": "9.0.0",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "prettier": "3.0.3",
    "scaffdog": "3.0.0",
    "turbo": "1.10.15"
  },
  "engines": {
    "node": ">=18.0.0 <19",
    "pnpm": ">=7.0.0"
  },
  "packageManager": "pnpm@8.8.0",
  "volta": {
    "node": "18.18.0"
  },
  "nextBundleAnalysis": {
    "budget": 358400,
    "budgetPercentIncreaseRed": 20,
    "minimumChangeThreshold": 0,
    "showDetails": true
  }
}